                        var jsType = "ABSTOCK";
                        //个股公告查询
                        function gggg_Search() {
                            var val = document.getElementById("ggdq_StockCode").value;
                            if (val == "输代码、名称或拼音" || val == "") {
                                alert("请输入所查询股票的代码!");
                                return;
                            }

                            //ab股是6位数字
                            var r = /^\d{6}$/;
                            //港股是5位数字
                            if (jsType == "HKSTOCK"){
                                r = /^\d{5}$/;
                            }
                            //美股是字母
                            else if (jsType == "USASTOCK") {
                                r = /^[a-z]+$/i;
                            }

                            if (r.test(val))
                                window.open("/stockeast/notices/detail?code=" + val);
                            else
                                alert("请输入所查询股票的代码!");
                        }
