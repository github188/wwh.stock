                        for (var i in data.CDSY_SECUCODES) {
                            if(data.CDSY_SECUCODES[i].SECURITYTYPECODE == stockTypeCode){
                                temp = data.CDSY_SECUCODES[i];
                                break;
                            }
                        }
                    }
                    if(temp){
                        code = temp.SECURITYCODE;
                        name = temp.SECURITYSHORTNAME;
                        marketCode = temp.TRADEMARKETCODE;
                    }
                }
                date = data.NOTICEDATE && data.NOTICEDATE.substr(0, data.NOTICEDATE.indexOf('T'));

                var hqLink = '';//行情链接
                var xgzlLink = '<a href="/notices/stock/' + code + '.html">更多公告</a>';//相关资料链接
                var detailLink = '/notices/detail/' + code + '/' + data.INFOCODE + ',' + btoa(encodeURIComponent(name)) + '.html';//公告正文链接

                if (market == 'gg') {
                    hqLink = 'http://quote.eastmoney.com/hk/' + code + '.html';
                    xgzlLink += ' <a href="http://guba.eastmoney.com/list,hk' + code + '.html">股吧</a> <a href="http://data.eastmoney.com/notice_n/reporthk_gs_' + code + '.html">研报</a>';
                }
                else if (market == 'mg') {
                    hqLink = 'http://quote.eastmoney.com/us/' + code + '.html';
                    xgzlLink += ' <a href="http://guba.eastmoney.com/list,us' + code + '.html">股吧</a>';
                    detailLink = 'http://pdf.dfcfw.com/pdf/H2_' + data.INFOCODE + '_1.html';
                }
                else {
                    xgzlLink += ' <a href="http://guba.eastmoney.com/list,' + code + '.html">股吧</a>';
                    if (market == 'sb'){
                        hqLink = 'http://quote.eastmoney.com/3ban/sz' + code + '.html';
                        xgzlLink += ' <a href="http://data.eastmoney.com/report/ReportSBStock.aspx?code=' + code + '">研报</a>';
                    }
                    else {
                        var m = marketCode.indexOf('069001001') >= 0 ? 'sh' : 'sz';
                        hqLink = 'http://quote.eastmoney.com/' + m + code + '.html';
                        xgzlLink += ' <a href="http://data.eastmoney.com/report/' + code + '.html">研报</a>';
                    }
                }

                if(market=="hsa" || market=="sha" ||market=="sza" ||market=="zxb" ||market=="cyb" ){
                    row.cells[0].innerHTML = '<a href="' + hqLink + '" class="hqPopCls" data_code="' + code+ '" data_name="' + name + '">' + code + '</a>';//股票代码
                    row.cells[1].innerHTML = '<a style="width:100px;" href="' + hqLink + '" title="' + name + '" class="hqPopCls"  data_code="' + code+ '" data_name="' + name + '">' + (name || '-') + '</a>';//股票简称
                }else{
                    row.cells[0].innerHTML = '<a href="' + hqLink + '" >' + code + '</a>';//股票代码
                    row.cells[1].innerHTML = '<a style="width:100px;" href="' + hqLink + '" title="' + name + '" >' + (name || '-') + '</a>';//股票简称

                }
                row.cells[2].innerHTML = xgzlLink;//相关资料链接
                row.cells[3].innerHTML = '<a class="overflow" style="text-align:left;width:350px" href="' + detailLink + '" title="' + data.NOTICETITLE + '">' + data.NOTICETITLE + '</a>';//公告标题
                if (market == 'mg') {
                    row.cells[4].innerHTML = '<span>' + date + '</span>';//公告日期
                }
                else {
                    row.cells[4].innerHTML = '<span style="width:100px;" class="overflow" title="' + typeName + '">' + typeName + '</span>';//公告类型
                    row.cells[5].innerHTML = '<span>' + date + '</span>';//公告日期
                }

            }
        });

        window.onload = function () {
            initDrawPopHq();
        }
    </script>
