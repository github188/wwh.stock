        function getColor(sj)
        {
            var span = "";
            sj = parseFloat(sj);
            if (sj > 0){
                span = '<span style="color:#f00">' + sj + '%</span>';
            }
            else if (sj < 0){
                span = '<span style="color:#090">' + sj + '%</span>';
            }
            else if (sj == 0){
                span = sj + "%";
            }
            return span;
        }

