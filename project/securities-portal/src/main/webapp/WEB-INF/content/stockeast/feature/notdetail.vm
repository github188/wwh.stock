                    c = "className";
                }
                var f10Li = document.getElementById("f10_tab").children;
                if (f10Li[showI].getAttribute(c) == "at")
                    return;

                f10Li[showI].setAttribute(c, "at");
                f10Li[hideI].setAttribute(c, "");

                var f10Ul = document.getElementById("f10-box").children;
                f10Ul[showI].style.display = "block";
                f10Ul[hideI].style.display = "none";
            }
            catch (e) { console && console.log(e); }
        }

