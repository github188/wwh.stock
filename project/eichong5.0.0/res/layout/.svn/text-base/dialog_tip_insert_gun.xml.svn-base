<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/common_white"
    android:padding="30dp" >
    
    <TextView 
         android:id="@+id/dialog_tip_insert_title"
         android:layout_width="match_parent"
    	 android:layout_height="wrap_content"
         android:textColor="@color/common_orange"
         android:textSize="20sp"
         android:paddingLeft="15dp"
         android:paddingRight="15dp"
         android:textStyle="bold"
         android:gravity="center"
        />
  
    <TextView 
        android:id="@+id/dialog_comment_success_confirm"
         android:layout_centerInParent="true"
         android:layout_below="@id/dialog_comment_title"
         android:layout_marginTop="30dp"
         android:layout_width="wrap_content"
    	 android:layout_height="wrap_content"
         android:text="取消"
         android:textColor="@color/common_white"
         android:textSize="17sp"
         android:gravity="center"
         android:paddingLeft="25dp"
         android:paddingRight="25dp"
         android:background="@drawable/bespoke_detail_led_gray"
        />
    
    
    
    
    

</RelativeLayout>
