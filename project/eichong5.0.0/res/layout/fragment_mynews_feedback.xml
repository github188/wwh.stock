<?xml version="1.0" encoding="utf-8"?>
<ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:background="@color/common_light_white"
    android:layout_height="match_parent">
    <LinearLayout 
        android:layout_width="match_parent"
    	android:layout_height="wrap_content"
    	android:orientation="vertical"
        >
         <TextView 
        android:id="@+id/fragment_mynews_feedback_nodata"
        android:layout_width="match_parent"
	    android:layout_height="wrap_content"
	    android:text="暂无我的反馈信息"
	    android:textColor="@color/common_black"
	    android:textSize="18sp"
        android:layout_marginTop="20dp"
        android:gravity="center"
        />
     <com.bm.wanma.view.MyDetailListView
        android:id="@+id/fragment_mynews_feedback_listview"
	    android:layout_width="match_parent"
	    android:layout_height="wrap_content"
	    android:divider="@null"
	    android:listSelector="@color/common_white"
	    ></com.bm.wanma.view.MyDetailListView>  
    </LinearLayout>
   

</ScrollView>
