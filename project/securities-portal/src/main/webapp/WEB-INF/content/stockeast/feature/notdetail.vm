            if (market != 'sb' && market != 'mg') {
                var favorLink = '${base}/stockeast/addstock?code=' + code;
                if(market == 'gg')
                    favorLink = 'http://quote.eastmoney.com/hk/favor/infavor.html?code=' + code;
