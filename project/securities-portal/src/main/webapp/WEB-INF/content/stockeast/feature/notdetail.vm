        //请求行情回调函数
        function hqCb(data) {
            //(0)市场 (1)代码 (2)名称 (3)收盘 (4)涨跌额 (5)涨跌幅% (6)成交量 (7)成交额 (8)振幅% (9)市盈率(动态) (10)换手率 (11)量比
            var str = '<td><a href="' + hqLink + '">' + code + '</a></td><td>-</td><td>-</td><td>-</td><td>-</td><td>-</td><td>-</td><td>-</td><td>-</td><td>-</td>';
            if (market != 'sb' && market != 'gg' && market != 'mg') {//三板、港股、美股 没有换手率和量比
                str += '<td>-</td><td>-</td>';
            }
            if (data && typeof data.stats != "boolean") {
                var arr = data.split(',');
                if(arr.length > 9)
                {
                    name = arr[2] || '-';
                    str = '<td><a href="' + hqLink + '">' + arr[1] + '</a></td>' //代码
                    + '<td><a href="' + hqLink + '">' + name + '</a></td>'; //名称
                    //<a>行情</a> <a>研报</a> <a>F10</a> <a>股吧</a>
                    str += '<td><a href="' + hqLink + '">行情</a>';
                    if (market == 'gg') {
                        str += ' <a href="http://data.eastmoney.com/notice_n/reporthk_gs_' + code + '.html">研报</a> <a href="http://hkf10.eastmoney.com/html_HKStock/index.html?securitycode=' + code + '">F10</a> <a href="http://guba.eastmoney.com/list,hk' + code + '.html">股吧</a>';
                    }
                    else if (market == 'mg') {
                        str += ' <a href="http://f10.eastmoney.com/usf10/gsjs.aspx?code=' + code + '">F10</a> <a href="http://guba.eastmoney.com/list,us' + code + '.html">股吧</a>';
                    }
                    else {
                        if (market == 'sb') {
                            str += ' <a href="http://data.eastmoney.com/report/ReportSBStock.aspx?code=' + code + '">研报</a>';
                        }
                        else {
                            str += ' <a href="http://data.eastmoney.com/report/' + code + '.html">研报</a> <a href="http://f10.eastmoney.com/HotNews.aspx?code=' + shORsz + code + '">F10</a>';
                        }
                        str += ' <a href="http://guba.eastmoney.com/list,' + code + '.html">股吧</a>';
                    }
                    str += '</td>';
                    var zde = parseFloat(arr[4]) || 0,
                            color = zde == 0 ? '' : (zde > 0 ? 'red' : 'green');
                    str += '<td><span class="' + color + '">' + arr[3] + '</span></td>'//最新价
                    + '<td><span class="' + color + '">' + arr[4] + '</span></td>'//涨跌额
                    + '<td><span class="' + color + '">' + arr[5] + '</span></td>'//涨跌幅
                    + '<td>' + arr[8] + '</td>'//振幅
                    + '<td>' + (market == 'gg' || market == 'mg' ? UFixAmt(arr[6], '万') : arr[6]) + '</td>'//成交量
                    + '<td>' + UFixAmt(arr[7], '万') + '</td>'//成交额
                    + '<td>' + arr[9] + '</td>';//市盈率
                    if (market != 'sb' && market != 'gg' && market != 'mg') {//三板、港股、美股 没有换手率和量比
                        str += '<td>' + (arr.length > 10 ? arr[10] : '-') + '</td><td>' + (arr.length > 11 ? arr[11] : '-') + '</td>';
