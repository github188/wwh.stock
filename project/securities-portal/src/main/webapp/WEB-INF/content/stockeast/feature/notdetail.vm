        function UFixAmt(_value, unit) {
            var v = parseFloat(_value) || 0;
            if (v == 0)
                return '-';
            if (unit == '亿') {
                return (v / 100000000).toFixed(2);
            } else if (unit == '万') {
                return (v / 10000).toFixed(2);
            } else {
                return v;
